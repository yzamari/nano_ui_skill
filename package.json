{
  "name": "nano-ui-skill",
  "version": "1.0.0",
  "description": "Claude Code plugin for generating unique design systems that don't look AI-generated",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nano-ui-mcp": "./dist/mcp/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist/",
    "prepare": "npm run build"
  },
  "keywords": [
    "claude-code-plugin",
    "design-system",
    "ui-ux",
    "gemini",
    "nano-banana",
    "anti-ai-look",
    "design-tokens",
    "mcp-server",
    "figma-tokens"
  ],
  "author": "Nano UI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nano-ui/nano-ui-skill"
  },
  "homepage": "https://nano-ui.dev",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  }
}
